/****************reset css starts here*****************/
h1, h2, h3, h4, h5, h6, p, ul, li, body, html, form, fieldset,figure { margin:0; padding:0; outline:none; font-weight:normal; border:0 }
form, fieldset { float:left; width:100% }
figure{float: left}
img { border:0 none; max-width: 100%; height: auto;display: inline-block;} 
a { text-decoration:none; border:0; outline:none!important;} 
ul { list-style:none; }
input,select,a,textarea{outline: none!important}
a:hover,a:visited,a:focus{text-decoration: none;}
.button:hover,.button:visited,.button:focus{color: #fff;}
.pad-left{padding-left: 0px;}
.pad-right{padding-right: 0px;}
.row{margin-left: 0px;margin-right: 0px;}
/****************reset css ends here*****************/
